<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/title_register"></include>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_65">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/logo_com"
                    android:textColor="@color/Black"
                    android:textSize="@dimen/sp_15"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:layout_centerVertical="true"/>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginRight="@dimen/dp_15"
                    android:layout_alignParentRight="true">
                    <com.cloud.shangwu.businesscloud.widget.CircleImageView
                        android:id="@+id/logo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/circle_error"
                        android:layout_centerVertical="true">
                    </com.cloud.shangwu.businesscloud.widget.CircleImageView>
                    <ImageView
                        android:layout_marginLeft="@dimen/dp_12"
                        android:layout_toRightOf="@id/logo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:src="@drawable/returnicon4"/>

                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_position"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_52">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/position_com"
                    android:textColor="@color/Black"
                    android:textSize="@dimen/sp_15"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_marginRight="@dimen/dp_15"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/returnicon4"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_companyint"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_52">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/companyint_com"
                    android:textColor="@color/Black"
                    android:textSize="@dimen/sp_15"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_marginRight="@dimen/dp_15"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/returnicon4"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/ll_busnisscope"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_52">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/busnissscope_com"
                    android:textColor="@color/Black"
                    android:textSize="@dimen/sp_15"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_marginRight="@dimen/dp_15"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/returnicon4"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_busnissgoal"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_52">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/busnissgoal_com"
                    android:textColor="@color/Black"
                    android:textSize="@dimen/sp_15"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:layout_marginRight="@dimen/dp_15"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/returnicon4"/>
            </RelativeLayout>

            <Button
                android:id="@+id/btn_register"
                android:layout_gravity="center_horizontal"
                android:layout_width="@dimen/dp_345"
                android:layout_height="42.5dp"
                android:background="@drawable/bt_register"
                android:textSize="@dimen/sp_17"
                android:textColor="@color/white"
                android:text="@string/register_now"/>

        </LinearLayout>
    </LinearLayout>

</android.support.constraint.ConstraintLayout>